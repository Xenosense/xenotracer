{
  "name": "xenotracer",
  "displayName": "Xenotracer",
  "description": "Trace and Visualize your Cairo contract file by using this extension!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "repository": "https://github.com/Xenosense/xeno-tracer/",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:xeno-tracer.visualizeContractFlow"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "xeno-tracer.visualizeContractFlow",
        "title": "XenoTracer: Visualize Contract Flow"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vscode/test-electron": "^2.1.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
